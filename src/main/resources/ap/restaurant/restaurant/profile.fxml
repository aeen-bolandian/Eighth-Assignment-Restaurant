<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ap.restaurant.restaurant.controller.profileController">
   <children>
      <Label fx:id="welcomeTextField" alignment="CENTER" layoutX="13.0" layoutY="14.0" prefHeight="60.0" prefWidth="575.0" style="-fx-background-radius: 20; -fx-background-color: white;">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <Button fx:id="logoutButton" layoutX="218.0" layoutY="252.0" mnemonicParsing="false" onAction="#logoutButtonAction" prefHeight="26.0" prefWidth="165.0" style="-fx-background-color: RED;" text="LOG OUT">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="orderListButton" layoutX="218.0" layoutY="185.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="165.0" style="-fx-background-color: YELLOW;" text="ORDER LIST">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="newOrderButton" layoutX="218.0" layoutY="119.0" mnemonicParsing="false" onAction="#newOrderButtonAction" prefHeight="26.0" prefWidth="165.0" style="-fx-background-color: GREEN;" text="ADD NEW ORDER">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
